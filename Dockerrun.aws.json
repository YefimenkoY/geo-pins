{
	"AWSEBDockerrunVersion": 1,
	"containerDefinitions": [
		{
			"name": "frontend",
			"image": "yurr92/frontend",
			"hostname": "frontend",
			"essential": false,
			"memory": 128
		},
		{
			"name": "backend",
			"image": "yurr92/backend",
			"hostname": "api",
			"essential": false,
			"memory": 128
		},
		{
			"name": "nginx",
			"image": "yurrr92/nginx",
			"hostname": "nginx",
			"essential": true,
			"memory": 128,
			"portMappings": [
				{
					"hostPort": 80,
					"containerPort": 80
				}
			],
			"links": ["frontend", "backend"]
		}
	]
}
